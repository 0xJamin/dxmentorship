---
import CtaFive from '../../components/newsletter/Newsletter.astro';
import BaseLayout from '../../layouts/BaseLayout.astro';
import { AstroSeo } from '@astrolib/seo';
import Community from '../../components/features/Community.astro';
import Newsletter from '../../components/newsletter/Newsletter.astro';
---

<AstroSeo
  title="DXMentorship Community"
  description="DXMentorship is a 12-week mentorship program for developers who want to start a career in developer advocacy."
  canonical="https://www.dxmentorship.com"
  openGraph={{
    url: 'https://www.dxmentorship.com',
    title: 'DXMentorship Community',
    description:
      'DXMentorship is a 12-week mentorship program for developers who want to start a career in developer advocacy.',
    images: [
      {
        url: '/opengraph/community-og.png',
        width: 1200,
        height: 630,
        alt: 'Social open graph',
        type: 'image/jpeg',
      },
      {
        url: '/opengraph/community-og.png',
        width: 4096,
        height: 4096,
        alt: 'Twitter open graph',
        type: 'image/jpeg',
      },
      { url: '/opengraph/community-og.png' },
      { url: '/opengraph/community-og.png' },
    ],
    site_name: 'DXMentorship',
  }}
  twitter={{
    handle: '@dxmentorship',
    site: '@site',
    cardType: 'summary_large_image',
  }}
/>
<BaseLayout>
  <Community />
  <!-- <Newsletter /> -->
</BaseLayout>
