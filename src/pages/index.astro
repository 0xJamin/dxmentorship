---
import BaseLayout from '../layouts/BaseLayout.astro';
import HeroTwo from '../components/heros/HeroTwo.astro';
import LogocCloudOne from '../components/testimonials/LogoCloudOne.astro';
import CustomerHome from '../components/customers/CustomerHome.astro';
import { AstroSeo } from '@astrolib/seo';
---

<!---
Use AstroSeo in all the pages you want different Seo than the index page
-->
<AstroSeo
  title="DxMentorship"
  description="Get equipped with knowledge and information that will propel you into a fulfilling Developer Advocacy career. We'll be working with industry experts and real world scenarios to prepare you for the job you've always wanted!"
  ,
  canonical="https://www.dxmentorship.com"
  openGraph={{
    url: 'https://www.dxmentorship.com',
    title: 'DxMentorship',
    description:
      "Get equipped with knowledge and information that will propel you into a fulfilling Developer Advocacy career. We'll be working with industry experts and real world scenarios to prepare you for the job you've always wanted!",
    images: [
      {
        url: 'https://www.lexingtonthemes.com/opengraph/social-og.jpg',
        width: 1200,
        height: 630,
        alt: 'Social open graph',
        type: 'image/jpeg',
      },
      {
        url: 'https://www.lexingtonthemes.com/opengraph/twitter-og.jpg',
        width: 4096,
        height: 4096,
        alt: 'Twitter open graph',
        type: 'image/jpeg',
      },
      { url: 'https://www.lexingtonthemes.com/opengraph/social-og.jpg' },
      { url: 'https://www.lexingtonthemes.com/opengraph/twitter-og.jpg' },
    ],
    site_name: 'DxMentorship',
  }}
  twitter={{
    handle: '@dxmentorship',
    site: '@site',
    cardType: 'summary_large_image',
  }}
/>

<BaseLayout>
  <HeroTwo />
  <LogocCloudOne />
  <CustomerHome />
</BaseLayout>
